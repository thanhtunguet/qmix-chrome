{
  "name": "English Dictionaries",
  "short_name": "English Dictionaries",
  "description": "English Dictionaries - A friend of English teachers",
  "version": "0.1.2",
  "manifest_version": 2,
  "permissions": [
    "https://dictionary.cambridge.org/**",
    "https://www.oxfordlearnersdictionaries.com/**"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://dictionary.cambridge.org/**"
      ],
      "js": [
        "build/cambridge.js"
      ]
    },
    {
      "matches": [
        "https://www.oxfordlearnersdictionaries.com/**"
      ],
      "js": [
        "build/oxford.js"
      ]
    }
  ],
  "options_page": "options.html",
  "page_action": {
    "default_icon": "icons/ed-32x32.png"
  },
  "background": {
    "scripts": [
      "build/background.js"
    ],
    "page": "background.html",
    "persistent": false
  },
  "icons": {
    "16": "icons/ed-16x16.png",
    "32": "icons/ed-32x32.png",
    "48": "icons/ed-48x48.png",
    "128": "icons/ed-128x128.png"
  },
  "web_accessible_resources": [
    "audio/*.mp3",
    "build/*.js",
    "build/*.js.map",
    "icons/",
    "images/",
    "options.html"
  ]
}
